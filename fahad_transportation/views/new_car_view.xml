                            <page string="Rents">
                                <notebook>
                                    <page string="Current Rent">
                                        <field name="rent_id" readonly="1" domain="[('state', '=', 'confirm')]"/>
                                    </page>
                                    <page string="History Rent">
                                        <field name="rent_history_id" readonly="1"/>
