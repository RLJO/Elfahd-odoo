<?xml version="1.0"?>
<odoo>
    <data>

        <!-- Workshop expenses -->
        <record model="ir.ui.view" id="expense_delivery_line_tree">
            <field name="name">expense.delivery.line tree</field>
            <field name="model">expense.delivery.line</field>
            <field name="arch" type="xml">
                <tree string="Workshop expenses" create="false">
                    <field name="date"/>
                    <field name="ref"/>
                    <field name="payment_method"/>
                    <field name="product_id"/>
                    <field name="quantity"/>
                    <field name="price"/>
                    <field name="total"/>
                    <field name="note"/>
                </tree>
            </field>
        </record>
<!--        <record model="ir.ui.view" id="expense_delivery_line_search">-->
<!--            <field name="name">expense.delivery.line search</field>-->
<!--            <field name="model">expense.delivery.line</field>-->
