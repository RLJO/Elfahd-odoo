                                        <field name="total_amount"/>
                                        <field name="discounted_aramco" string="‫‪Deduction‬‬ ‫‪Aramco‬‬" sum="1"/>
                                        <field name="net_amount" sum="1"/>
                                        <field name="diff_liter" invisible="1"/>
